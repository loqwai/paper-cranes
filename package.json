{
  "name": "paper-cranes-audio",
  "version": "1.0.2",
  "description": "",
  "type": "module",
  "main": "dist/src/audio/AudioProcessor.js",
  "scripts": {
    "start": "npm run dev",
    "serve": "live-server --wait=50 -q --port=6969 ./dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./esbuild.js",
    "dev": "npm run build && concurrently \"npm run serve\" \"npm run build:watch\"",
    "build:watch": "chokidar '**/*.*' -i 'node_modules/*' -i 'dist/**/*.*' -c 'npm run build'",
    "format": "eslint --fix ."
  },
  "author": "hypnodroid",
  "license": "UNLICENSED",
  "dependencies": {
    "@preact/signals": "^1.3.1",
    "debounce": "^2.2.0",
    "htm": "^3.1.1",
    "html": "^1.0.0",
    "hypnosound": "^1.8.0",
    "preact": "^10.25.4",
    "twgl-base.js": "^5.5.3"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "esbuild": "^0.20.2",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-css-modules": "^0.3.0",
    "esbuild-plugin-html": "^1.0.0",
    "eslint": "^8.57.1",
    "live-server": "^1.2.2",
    "mini-css-extract-plugin": "^2.9.2",
    "ncp": "^2.0.0",
    "prettier": "^3.4.2"
  },
  "languages": [
    {
      "id": "paper-cranes-fragment-shader",
      "aliases": [
        "GLSL",
        "glsl"
      ],
      "extensions": [
        ".frag",
        ".vert",
        ".glsl"
      ]
    }
  ]
}
